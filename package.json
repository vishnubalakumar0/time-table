{
  "name": "timetable",
  "version": "1.0.0",
  "description": "Smart Timetable Manager",
  "scripts": {
    "start": "parcel public/index.html",
    "build": "parcel build public/index.html --dist-dir dist --public-url ./",
    "android": "npm run build && npx cap sync android && npx cap open android",
    "android:dev": "npx cap run android -l --external",
    "android:debug": "npm run build && npx cap sync android && cd android && .\\gradlew assembleDebug",
    "android:release": "npm run build && npx cap sync android && cd android && .\\gradlew bundleRelease",
    "build:android": "npm run build && npx cap copy && npx cap sync",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "timetable",
    "education",
    "schedule"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@capacitor/android": "^5.7.8",
    "@capacitor/core": "^5.7.8",
    "firebase": "^10.7.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.7.8",
    "@parcel/transformer-react-refresh-wrap": "^2.11.0",
    "buffer": "^6.0.3",
    "parcel": "^2.11.0"
  }
}
